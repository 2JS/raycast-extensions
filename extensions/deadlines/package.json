{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "deadlines",
  "title": "Deadlines",
  "description": "Manage your deadlines",
  "icon": "extension_icon.png",
  "author": "thomaslombart",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "add-deadline",
      "title": "Add Deadline",
      "description": "Add a new deadline.",
      "mode": "view"
    },
    {
      "name": "my-deadlines",
      "title": "My Deadlines",
      "description": "See all your deadlines.",
      "mode": "view"
    },
    {
      "name": "menu-bar",
      "title": "Menu Bar Deadlines",
      "description": "See your deadlines in the menu bar.",
      "mode": "menu-bar"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1",
    "@raycast/utils": "^1.10.1",
    "date-fns": "^3.2.0",
    "lodash": "^4.17.21",
    "node-emoji": "^2.1.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
